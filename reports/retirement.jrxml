	<queryString language="SQL">
		<![CDATA[SELECT
    a.item_code,
    b.item_name,
    a.quantity,
    a.remarks,
    a.date_retirement
FROM
    tbl_retirement a
LEFT JOIN tbl_item b ON
    a.item_code = b.item_code
WHERE
    DATE(a.date_retirement) BETWEEN DATE('$P{fromdate}') AND DATE('$P{todate}')
ORDER BY
    a.date_retirement
DESC
    ]]>
	</queryString>
